<% include _header %>
<h1> Welcome to Learn About Me! </h1>

<% for( var i = 0; i < users.length; i++) { %>
    <% var user = users[i] %>
    <div class="panel panel-default">
        <div class="panel-heading">
            <a href="/users/<%= user.username %>">
                <%= user.name() %>
            </a>
        </div>
        <% if (user.bio) { %>
            <div class="panel-body"><%= user.bio %></div>
        <% } %>
    </div>
<% } %>

<% include _footer %>